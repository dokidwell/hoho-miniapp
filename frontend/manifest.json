{
  "name": "HOHO",
  "appid": "",
  "description": "HOHO数字藏品集换小程序",
  "version": "1.0.0",
  "versionCode": 100,
  "transformPx": false,
  "app-plus": {
    "usingComponents": true,
    "nvueCompiler": "uni-app",
    "compilerVersion": 3,
    "splashscreen": {
      "alwaysShowBeforeRender": true,
      "waiting": 0,
      "autoclose": true,
      "delay": 0
    },
    "modules": {},
    "distribute": {
      "android": {
        "permissions": [
          "CHANGE_NETWORK_STATE",
          "MOUNT_UNMOUNT_FILESYSTEMS",
          "VIBRATE",
          "WRITE_SETTINGS",
          "READ_PHONE_STATE",
          "CHANGE_WIFI_STATE",
          "ACCESS_NETWORK_STATE",
          "ACCESS_WIFI_STATE",
          "READ_LOGS",
          "WRITE_LOGS",
          "ACCESS_COARSE_LOCATION",
          "ACCESS_FINE_LOCATION",
          "READ_EXTERNAL_STORAGE",
          "WRITE_EXTERNAL_STORAGE",
          "INTERNET"
        ]
      },
      "ios": {},
      "sdkConfigs": {}
    }
  },
  "h5": {
    "title": "HOHO数字藏品集换",
    "router": {
      "mode": "history",
      "base": "/"
    },
    "devServer": {
      "port": 8081,
      "disableHostCheck": true,
      "proxy": {
        "/api": {
          "target": "http://localhost:8080",
          "changeOrigin": true,
          "pathRewrite": {
            "^/api": "/api"
          }
        }
      }
    }
  },
  "mp-weixin": {
    "appid": "",
    "setting": {
      "urlCheck": false,
      "es6": true,
      "postcss": true,
      "minified": true,
      "lazyloadPlaceHolderEnable": false
    },
    "usingComponents": true,
    "permission": {}
  },
  "mp-alipay": {
    "usingComponents": true
  },
  "mp-toutiao": {
    "usingComponents": true
  },
  "quickapp": {},
  "mp-qq": {
    "appid": "",
    "usingComponents": true
  },
  "mp-baidu": {
    "appid": "",
    "usingComponents": true
  },
  "mp-kuaishou": {
    "appid": "",
    "usingComponents": true
  },
  "mp-jd": {
    "appid": "",
    "usingComponents": true
  },
  "mp-360": {
    "appid": "",
    "usingComponents": true
  }
}
